{
  "loginUsernameBox": "input[name=username]",
  "loginPasswordBox": "input[name=password]",
  "loginSubmit": "#d > div.panel-footer > button",

  "offerAdd": "#f > div:nth-child(3) > div.btn-group > button.btn.btn-success",
  "offerSell": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(1) > span",
  "offerSellQuantity": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(1) > span:nth-child(1) > span:nth-child(2)",
  "offerSellItems": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(1)",
  "offerBuy": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(2) > span",
  "offerBuyQuantity": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(2) > span:nth-child(1) > span:nth-child(2)",
  "offerBuyItems": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(2)",
  "offerVolume": "#f > table:nth-child(2) > tbody > tr:nth-child(%index%) > td:nth-child(3) > span",
  "offerSuspendAll": "#f > div:nth-child(3) > div.btn-group > button.btn.btn-warning",
  "offerUnsuspendAll": "#f > div.btn-group > button.btn.btn-success",
  "offerRemoveActive": "#f > div:nth-child(3) > div.btn-group > button.btn.btn-danger",
  "offerRemoveSuspended": "#f > div.btn-group > button.btn.btn-danger",
  "offerSave": "#f > div:nth-child(3) > button",
  "offerActiveOffers": "#f > table:nth-child(2) > tbody > tr",
  "offerSuspendedOffers": "#f > table:nth-child(5) > tbody > tr",
  "offerSuspendedSellItems": "#f > table:nth-child(5) > tbody > tr:nth-child(%index%) > td:nth-child(1)",
  "offerSuspendedBuyItems": "#f > table:nth-child(5) > tbody > tr:nth-child(%index%) > td:nth-child(2)",

  "offerSuspendAction": "el => el.lastChild.firstChild.children[1].click()",
  "offerUnsuspendAction": "el => el.lastChild.firstChild.firstChild.click()",
  "offerFetchSellItemIdAction": "el => el.children[%index%].lastChild.getAttribute('data-item')",
  "offerFetchSellItemQuantityAction": "el => el.children[%index%].children[1].innerText",
  "offerSuspendedFetchSellItemIdAction": "el => el.firstChild.firstChild.getAttribute('data-item')",
  "offerSuspendedFetchSellItemQuantityAction": "el => el.firstChild.lastChild.innerText",
  "offerFetchBuyItemIdAction": "el => el.children[%index%].lastChild.getAttribute('data-item')",
  "offerFetchBuyItemQuantityAction": "el => el.children[%index%].children[1].innerText",
  "offerSuspendedFetchBuyItemIdAction": "el => el.firstChild.firstChild.getAttribute('data-item')",
  "offerSuspendedFetchBuyItemQuantityAction": "el => el.firstChild.lastChild.innerText",
  "offerFetchVolumeAction": "el => el.children[2].firstChild.innerText",
  "offerSuspendedFetchVolumeAction": "el => el.children[2].innerText",
  "itemsFetchItemIdAction": "el => el.firstElementChild.getAttribute('data-item')",

  "realmeyeItems": "#item-selector > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > span"
}
